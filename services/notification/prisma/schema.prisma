generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model Notification{
    id String @id @default(uuid())
    userId String
    title String
    message String
    type String
    read Boolean @default(false)
    createdAt DateTime @default(now())
    sourceEventId String? @unique

    @@index([userId,read])
}